{
    "ollama_endpoint": "http://localhost:11434",
    "model": "llama3.2:latest",
    "context_window": 128000,
    "temperature": 0.7,
    "max_tokens": 8000,
    "history_file": ".ollamacode_history",
    "system_prompt_file": "ollamacode/claude_style_system_prompt.py",
    "enable_bash": true,
    "enable_tools": true,
    "safe_mode": true,
    "working_directory": "ollamacode_workspace",
    "allowed_tools": [
      "file_read", 
      "file_write", 
      "file_list", 
      "file_search", 
      "file_grep", 
      "edit", 
      "web_get", 
      "sys_info", 
      "python_run", 
      "bash", 
      "batch"
    ],
    "auto_extract_code": false,
    "auto_save_code": false,
    "auto_run_python": false,
    "code_directory": "",
    "process_followup_commands": true,
    "max_followup_depth": 2,
    "use_function_calling": true,
    "fallback_to_legacy": true,
    "replace_function_calls": true,
    "claude_code_style": true
  }